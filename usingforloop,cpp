#include <iostream>
using namespace std;

int main() {
  float num, sum = 0;
  double avg;

  cout << "Enter 5 numbers for finding the average:" << endl;

  for (int i = 0; i < 5; i++) {
    cin >> num;
    sum = sum + num;
  }
  avg = sum / 5;
  cout << "\n The average of 5 numbers is: " << avg << endl;
  return 0;
}
